{
  "$schema": "https://schemas.wp.org/wp/6.8/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "defaultPalette": true,
      "defaultGradients": false,
      "defaultDuotone": false,
      "custom": true,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "palette": [
        {
          "name": "Neutral-Off-white",
          "slug": "neutral-off-white",
          "color": "#FFFCF9"
        },
        {
          "name": "Neutral-black",
          "slug": "neutral-black",
          "color": "#111121"
        },
        {
          "name": "Neutral-white",
          "slug": "neutral-white",
          "color": "#ffffff"
        },
        {
          "name": "Neutral-contrast",
          "slug": "neutral-contrast",
          "color": "#F2EFED"
        },
        {
          "name": "neutral-gray",
          "slug": "neutral-gray",
          "color": "#C9C9C9"
        },
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#F6652B"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#FEEEC2"
        },
        {
          "name": "Tertiary",
          "slug": "tertiary",
          "color": "#FFD7CA"
        },
        {
          "name": "Accent",
          "slug": "accent",
          "color": "#FCF5F2"
        }
      ]
    },
    "custom": {},
    "layout": {
      "contentSize": "960px",
      "wideSize": "1290px"
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "units": ["px", "em", "rem", "%", "vh", "vw"]
    },
    "typography": {
      "customFontSize": false,
      "lineHeight": false,
      "dropCap": false,
      "fontSizes": [
        {
          "name": "Small",
          "slug": "small",
          "size": "1rem",
          "fluid": {
            "min": "1rem",
            "max": "1.25rem"
          }
        },
        {
          "name": "Normal",
          "slug": "normal",
          "size": "1.25rem",
          "fluid": false
        },
        {
          "name": "Medium",
          "slug": "medium",
          "size": "2rem",
          "fluid": {
            "min": "1.5rem",
            "max": "2rem"
          }
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "2.5rem"
        },
        {
          "name": "Extra Large",
          "slug": "extra-large",
          "size": "4rem",
          "fluid": {
            "min": "2.25rem",
            "max": "4rem"
          }
        },
        {
          "name": "XX Large",
          "slug": "xx-large",
          "size": "5rem",
          "fluid": {
            "min": "2.25rem",
            "max": "5rem"
          }
        }
      ],
      "fluid": true,
      "fontFamilies": [
        {
          "name": "Chillax",
          "slug": "chillax",
          "fontFamily": "'Chillax', sans-serif",
          "fontFace": [
            {
              "fontFamily": "'Chillax', sans-serif",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": "file:./assets/fonts/chillax/Chillax-Regular.otf"
            },
            {
              "fontFamily": "'Chillax', sans-serif",
              "fontWeight": "600",
              "fontStyle": "normal",
              "src": "file:./assets/fonts/chillax/Chillax-Semibold.otf"
            },
            {
              "fontFamily": "'Chillax', sans-serif",
              "fontWeight": "700",
              "fontStyle": "normal",
              "src": "file:./assets/fonts/chillax/Chillax-Bold.otf"
            }
          ]
        },
        {
          "name": "Satoshi",
          "slug": "satoshi",
          "fontFamily": "'Satoshi', sans-serif",
          "fontFace": [
            {
              "fontFamily": "'Satoshi', sans-serif",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": "file:./assets/fonts/satoshi/Satoshi-Regular.otf"
            },
            {
              "fontFamily": "'Chillax', sans-serif",
              "fontWeight": "700",
              "fontStyle": "normal",
              "src": "file:./assets/fonts/satoshi/Satoshi-Bold.otf"
            }
          ]
        }
      ]
    },
    "blocks": {
      "core/button": {
        "color": {
          "palette": []
        }
      }
    }
  },
  "styles": {
    "color": {},
    "elements": {
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--extra-large)",
          "fontWeight": "600",
          "lineHeight": "1"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600",
          "lineHeight": "1"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "1"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "1"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "1"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "1"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--satoshi)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "1.5",
      "letterSpacing": "0.8px"
    },
    "blocks": {}
  }
}
