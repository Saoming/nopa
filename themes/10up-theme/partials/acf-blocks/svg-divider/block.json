{
  "name": "acf/svg-divider-block",
  "title": "SVG Divider Block",
  "description": "A custom svg divider block that uses ACF fields.",
  "style": "file:../../../assets/css/blocks/svg-divider.css",
  "category": "theme",
  "icon": "admin-comments",
  "apiVersion": 2,
  "keywords": ["svg", "divider", "separator", "block"],
  "acf": {
    "mode": "preview",
    "renderTemplate": "svg-divider.php",
    "postTypes": ["post", "page"]
  },
  "styles": [
    { "name": "default", "label": "Default", "isDefault": true },
    { "name": "style2", "label": "Style2" }
  ],
  "supports": {
    "align": true,
    "alignContent": false,
    "spacing": {
      "margin": ["top", "bottom"],
      "padding": true
    },
    "attributes": {
      "align": {
        "type": "string",
        "default": "full"
      }
    }
  }
}
